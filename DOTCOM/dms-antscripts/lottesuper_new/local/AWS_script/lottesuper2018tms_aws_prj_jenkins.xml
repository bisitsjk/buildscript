	<target name="check_exist"> 	<!--source type check-->
		<condition property="isExistClass">
			<or>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/dynamic_classes"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/classes"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/lib"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/x2config"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/x2scenario"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/config.xml"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/config-dev.xml"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/config-test.xml"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/config-real.xml"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/jeus-web-dd.xml"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/jeus-web-dd-dev.xml"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/jeus-web-dd-test.xml"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/jeus-web-dd-real.xml"/></resourceexists>
			<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/web.xml"/></resourceexists>
			</or>
		</condition>
		<condition property="isExistJsp">
			<or>
				<resourceexists><file file="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms/WEB-INF/jsp"/></resourceexists>
				<resourcecount when="greater" count="0">
					<filelist dir="${hdsnWrkSpPath}/${JOB_NAME}/workspace/lottesuper2011/${hdsnPackPath}/${prj.name}/${deploy.id}/project/tms" files="*.jsp,*.html"/>
				</resourcecount>				
			</or>
		</condition>
		
		<condition property="isJspAndClass">
			<and>
				<istrue value="${isExistClass}"/>
				<istrue value="${isExistJsp}"/>
			</and>
		</condition>
		<condition property="isJsp">
			<and>
				<isfalse value="${isExistClass}"/>
				<istrue value="${isExistJsp}"/>
			</and>
		</condition>
		<condition property="isClass">
			<and>
				<istrue value="${isExistClass}"/>
				<isfalse value="${isExistJsp}"/>
			</and>
		</condition>
		<condition property="isExist">
			<or>
				<istrue value="${isExistClass}"/>
				<istrue value="${isExistJsp}"/>
			</or>
		</condition>		
		<condition property="isRestart">
			<or>
				<istrue value="${isJspAndClass}"/>
				<istrue value="${isClass}"/>
			</or>
		</condition>
		<echo>isJsp : ${isJsp} </echo>
		<echo>isClass : ${isClass} </echo>
		<echo>isJspAndClass : ${isJspAndClass} </echo>
		<echo>jeus restart - >> ${isRestart}</echo>
	</target>
		
