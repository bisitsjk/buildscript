<?xml version="1.0" encoding="UTF-8"?>
<project name="lottesuper2011ww_jenkins">
    <!-- **************************************************************** -->
	<!-- PROPERTY -->
	<!-- **************************************************************** -->
	<property name="deploy.id" value="${DEPLOY_ID}"/>
	<property name="comm.deploy.id" value="${COMM_DEPLOY_ID}"/>
	<property file="${deployer.home.dir}/scripts/${ant.project.name}.${deploy.phase}.properties"/>
	<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin -->
	<property name="pack.real.path" value="${hdsnWrkSpPath}/${svnReposNm}/${hdsnPackPath}/${deploy.id}/${svrWebappPath}" />
	<property name="comm.wrksp.path" value="E:/jenkins-lottesuper/jobs/LOTTESUPER_COMMONS_1_TEST_BUILD/workspace" />
	<property name="comm.lib.path" value="lotte-super-commons/lib" />
	<property name="comm.jar.path" value="lotte-super-commons/dist" />
	<property name="comm.pack.lib.path" value="project/back/WEB-INF/lib" />
	<property name="user.id" value="${svrUserId}"/>
	<property name="user.pwd" value="${svrUserPassword}"/>
	
	<property name="init.build.log" value="../logs/${ant.project.name}/${deploy.id}_BUILD.log" />
	<property name="init.was.log" value="../logs/${ant.project.name}/${deploy.id}_INIT_WAS.log" />
	<property name="init.web.log" value="../logs/${ant.project.name}/${deploy.id}_INIT_WEB.log" />
	<property name="dep.was.log" value="../logs/${ant.project.name}/${deploy.id}_DEP_WAS.log" />
	<property name="dep.web.log" value="../logs/${ant.project.name}/${deploy.id}_DEP_WEB.log" />
	<property name="rollback.was.log" value="../logs/${ant.project.name}/${deploy.id}_ROLLBACK_WAS.log" />
	<property name="jeus.restart.log" value="../logs/${ant.project.name}/${deploy.id}_JEUS_RESTART.log" />
	
	<property name="init.cmd" value="wwinit" />
	<property name="dep.cmd" value="wwdeploy" />

	<property name="ssh.init.cmd" value="ssh_wwinit" />
	<property name="ssh.dep.cmd" value="ssh_wwdeploy" />
	<property name="ssh.dep.test.cmd" value="ssh_wwdeploytest" />
	<property name="ssh.rollback.cmd" value="ssh_wwrollback" />
	<property name="ssh.rollback.test.cmd" value="ssh_wwrollbacktest" />

		
	<!--<property name="con_winwin"     value="esptstf_winwindev" />-->

	<!-- 시험배포 자동화용 -->
	<tstamp><format property="deploy.date" pattern="yyyyMMdd"/></tstamp>
	<property name="rglr.dep.id" value="WW_${deploy.date}_1330"/>

	<!-- 첫 접수시 0건이면, 다음 접수시 지각한 건 접수하지 않도록 하기위함 -->
	<property name="lock.workspace" value="E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_0_TEST_LOCK/workspace" />
		
	<!-- properties -->
	<!-- hudson setting properties
		deployer.home.dir
		java.home.dir
		jeus.home.dir
		deploy.phase
	-->
	
	<target name="run_build">
		<antcall target="merge" />
		<antcall target="build" />
		<antcall target="pack" />
	</target>
	
	<target name="run_deploy">
		<antcall target="backup_was" />
		<antcall target="deploy_was" />
		<antcall target="copy_was" />
	</target>
	
	<target name="aws_build">
		<antcall target="delete.pack" />
		<antcall target="delete.s3" />
		<antcall target="deploy.copy" />
		<antcall target="s3.copy" />
		<antcall target="sh.replace" />
	</target>
	
	<!-- **************************************************************** -->
	<!-- CHECK -->
	<!-- **************************************************************** -->
	<target name="ip_check">
	<echo>echo ${was.ip}</echo>
	  <condition property="winwin_tom" value="winwin11">
	   		<equals arg1="${was.ip}" arg2="10.149.199.31" />
	  </condition>

	  <condition property="winwin_tom" value="winwin21">
		   <equals arg1="${was.ip}" arg2="10.149.199.46" />
	  </condition>
	</target>

		
	<target name="check_common">
		<echo>♠ 공통 배포ID가 있는지 확인합니다.</echo>
		<condition property="existCommon">
			<and>
				<contains string="${comm.deploy.id}" substring="CM_" casesensitive="true" />
				<length string="${comm.deploy.id}" when="greater" length="15"/>
			</and>
		</condition>
	</target>
	
	<!-- 2013-01-02 변경 -->
<!-- jeus-web-dd-test.xml or jeus-web-dd-real.xml 파일이 존재하는지 Check -->
	<target name="check_rename2">
		<condition property="existFile2">
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/jeus-web-dd-real.xml-->
			<resourceexists><file file="${pack.real.path}/WEB-INF/jeus-web-dd-${deploy.phase}.xml" /></resourceexists>
		</condition>
		<echo>config-${deploy.phase}.xml : ${existFile2}</echo>
	</target>

<!-- x2config-test.xml or x2config-real.xml 파일이 존재하는지 Check -->
	<target name="check_rename3">
		<condition property="existFile3">
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/x2config-real.xml-->
			<resourceexists><file file="${pack.real.path}/WEB-INF/x2config/x2config-${deploy.phase}.xml" /></resourceexists>
		</condition>
		<echo>x2config-${deploy.phase}.xml : ${existFile3}</echo>
	</target>

<!-- log4jconfig-test.xml or log4jconfig-real.xml 이 존재하는지 Check -->
	<target name="check_rename4">
		<condition property="existFile4">
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/log4jconfig-real.xml-->
			<resourceexists><file file="${pack.real.path}/WEB-INF/x2config/log4jconfig-${deploy.phase}.xml" /></resourceexists>
		</condition>
		<echo>log4jconfig-${deploy.phase}.xml : ${existFile4}</echo>
	</target>

	<target name="check_remote_work" unless="isSuccess">
		<fail message="원격지 작업이 실패하였습니다..." />
	</target>
	
	<target name="check_lock_prop">
		<condition property="existLockProp">
			<resourceexists>
				<file file="${lock.workspace}/${rglr.dep.id}_lock.properties"/>
			</resourceexists>
		</condition>
	</target>	
	
	<target name="check_exist">		<!--source type check-->
		<!-- 아래 파일들중 하나라도 있으면 class 파일로 재기동 필요 -->
		<condition property="isExistClass">
			<or>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/dynamic_classes -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/dynamic_classes"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/classes -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/classes"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/lib -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/lib"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/x2config -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/x2config"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/x2scenario -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/x2scenario"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/config.xml -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/config.xml"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/config-dev.xml -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/config-dev.xml"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/config-test.xml -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/config-test.xml"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/config-real.xml -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/config-real.xml"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/jeus-web-dd.xml -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/jeus-web-dd.xml"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/jeus-web-dd.dev.xml -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/jeus-web-dd-dev.xml"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/jeus-web-dd.test.xml -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/jeus-web-dd-test.xml"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/jeus-web-dd.real.xml -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/jeus-web-dd-real.xml"/></resourceexists>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/web.xml -->
			<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/web.xml"/></resourceexists>
			</or>
		</condition>

		<!-- 아래 파일들중 하나라도 있으면 jsp, html 파일로 재기동 불필요 -->
		<condition property="isExistJsp">
			<or>
				<!-- 아래 2개중에 하나라도 해당된다면 -->
				<!-- jsp 폴더가 있다면이라는 뜻인지, 아니면 jsp 폴더안에 파일들이 있느냐라는 뜻인지(?) -->
				<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin/WEB-INF/jsp -->
				<resourceexists><file file="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin/WEB-INF/jsp"/></resourceexists>
				<resourcecount when="greater" count="0">
					<!-- DocRoot에 .jsp , .html 파일이 있다면(?) -->		
					<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/pack/REAL/WW_20160509_1330/project/winwin -->
					<filelist dir="${hdsnWrkSpPath}/lottesuper2011/${hdsnPackPath}/${deploy.id}/project/winwin" files="*.jsp,*.html"/>
				</resourcecount>				
			</or>
		</condition>
		
		<!-- jsp , class 둘다 있는 경우 -->
		<condition property="isJspAndClass">
			<and>
				<istrue value="${isExistClass}"/>
				<istrue value="${isExistJsp}"/>
			</and>
		</condition>

		<!-- jsp만 있는 경우 -->
		<condition property="isJsp">
			<and>
				<isfalse value="${isExistClass}"/>
				<istrue value="${isExistJsp}"/>
			</and>
		</condition>

		<!-- class 만 있는 경우 -->
		<condition property="isClass">
			<and>
				<istrue value="${isExistClass}"/>
				<isfalse value="${isExistJsp}"/>
			</and>
		</condition>

		<!-- jsp 혹은 class 파일이 있는 경우 -->
		<condition property="isExist">
			<or>
				<istrue value="${isExistClass}"/>
				<istrue value="${isExistJsp}"/>
			</or>
		</condition>

		<!-- 
		## 재기동이 필요한 경우
			1. jsp 혹은 class 파일이 있는 경우
			2. class 파일만 있는 경우
		-->
		<condition property="isRestart">
			<or>
				<istrue value="${isJspAndClass}"/>
				<istrue value="${isClass}"/>
			</or>
		</condition>
		<echo>isJsp : ${isJsp} </echo>
		<echo>isClass : ${isClass} </echo>
		<echo>isJspAndClass : ${isJspAndClass} </echo>
		<echo>jeus restart - >> ${isRestart}</echo>
	</target>

	<!-- **************************************************************** -->
	<!-- LOCK, 접수 : 정기배포, 자동접수 시에만 사용 -->
	<!-- **************************************************************** -->
	<target name="set_prop" unless="existLockProp">
		<propertyfile file="${lock.workspace}/${rglr.dep.id}_lock.properties">
			<entry key="deploy_count" type="int" value="0" />
			<entry key="first_error" value="false" />
		</propertyfile>
	</target>
		
	<target name="exec_app">
		<property file="${lock.workspace}/${rglr.dep.id}_lock.properties" />
		
		<condition property="isStop">
			<equals arg1="${first_error}" arg2="true" trim="true" />
		</condition>
		<fail if="${isStop}" message="당일 접수한 배포 건이 없습니다." />		
		
		<java jvm="${java.home.dir}/bin/java" fork="yes"
		      failonerror="false"
		      jar="${deployer.home.dir}/bin/deploy-deployer.jar"
			  errorproperty="error">
			<arg value="${ant.project.name}"/>
			<arg value="${rglr.dep.id}"/>
			<arg value="${deploy.phase}"/>
			<arg value="accept"/>
		</java>
		<condition property="isError">
			<length string="${error}" when="greater" length="0" />
		</condition>
		
		<condition property="isFirstError">
			<and>
				<istrue value="${isError}" />
				<equals arg1="0" arg2="${deploy_count}" trim="true" />
			</and>
		</condition>
		
		<propertyfile file="${lock.workspace}/${rglr.dep.id}_lock.properties">
			<entry key="deploy_count" default="0" type="int" operation="+" />
			<entry key="first_error" value="${isFirstError}" />
		</propertyfile>
		
		<fail if="${isError}" message="." />
		
		<record name="${ant.project.name}_param.properties" emacsmode="true" action="start" />
		<echo>DEPLOY_ID=${rglr.dep.id}</echo>
		<record name="${ant.project.name}_param.properties" emacsmode="true" action="stop" />
	</target>
	
	<target name="accept" depends="check_lock_prop">
		<antcall target="set_prop" />
		<antcall target="exec_app" />
	</target>
			
	<!-- **************************************************************** -->	
	<!-- MERGE -->
	<!-- **************************************************************** -->
	<target name="merge">
		<java jvm="${java.home.dir}/bin/java" fork="true"
		      failonerror="true"
		      jar="${deployer.home.dir}/bin/deploy-deployer.jar">
			<arg value="${ant.project.name}"/>
			<arg value="${deploy.id}"/>
			<arg value="${deploy.phase}"/>
			<arg value="merge"/>
		</java> 
	</target>
	
	<!-- **************************************************************** -->
	<!-- BUILD -->
	<!-- **************************************************************** -->
	<target name="build">
		<record name="${init.build.log}" action="start"/>
		<!-- 빌드에 필요한 추가정보 획득  -->
		<!-- 파일로 저장 -->
		<java jvm="${java.home.dir}/bin/java" fork="true"
		      failonerror="true"
		      jar="${deployer.home.dir}/bin/deploy-deployer.jar">
			<arg value="${ant.project.name}"/>
			<arg value="${deploy.id}"/>
			<arg value="${deploy.phase}"/>
			<arg value="build"/>
		</java>		
		<!-- 추가정보가 담겨있는 파일에서 값을 획득 -->
		<!-- 왜 info 파일에서 추가정보를 가져오는가? 
			 config 파일에서 Multi 로 가져오지 못해서 그런것인가?
			 Build Java 소스에 보면 Config 파일에서 Multi 경로를 가져온다.
		-->
		
		<property file="${hdsnWrkSpPath}/${svnReposNm}/${deploy.id}.info"/>
		<!--
			commYn=N
			srcJavaPath0=lotte-super-front/src_dynamic
			srcJavaPath1=lotte-super-front/src
			srcClassesPath0=lotte-super-front/WebContent/WEB-INF/dynamic_classes
			srcClassesPath1=lotte-super-front/WebContent/WEB-INF/classes
		-->
		<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/branches/REAL/lotte-super-winwin/src -->
		<property name="srcStaticJavaPath" value="${hdsnWrkSpPath}/${svnReposNm}/${hdsnBranchPath}/${srcJavaPath1}" />
		<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/branches/REAL/lotte-super-winwin/src_dynamic -->
		<property name="srcDynamicJavaPath" value="${hdsnWrkSpPath}/${svnReposNm}/${hdsnBranchPath}/${srcJavaPath0}" />
		<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/branches/REAL/lotte-super-winwin/WebContent/WEB-INF/classes -->
		<property name="srcStaticClassesPath" value="${hdsnWrkSpPath}/${svnReposNm}/${hdsnBranchPath}/${srcClassesPath1}" />
		<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/branches/REAL/lotte-super-winwin/WebContent/WEB-INF/dynamic_classes -->
		<property name="srcDynamicClassesPath" value="${hdsnWrkSpPath}/${svnReposNm}/${hdsnBranchPath}/${srcClassesPath0}" />
				
		<path id="classpath">
			<fileset dir="${java.home.dir}/jre/lib">
				<include name="rt.jar" />
			</fileset>
			
			<!--
			<fileset dir="${jeus.home.dir}/lib/datasource">
				<include name="*.jar" />
			</fileset>
			<fileset dir="${jeus.home.dir}/lib/system">
				<include name="*.jar" />
			</fileset>
			-->
			
			<!-- Tomcat Library Reference -->
			
			<fileset dir="${tomcat.home.dir}/lib">
				<include name="*.jar" />
			</fileset>
			

			
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_COMMONS_1_TEST_BUILD/workspace/lottesuper2011/branches/REAL/lotte-super-commons/lib/*.jar-->
			<fileset dir="${comm.wrksp.path}/${svnReposNm}/${hdsnBranchPath}/${comm.lib.path}">
				<include name="*.jar" />
			</fileset>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_COMMONS_1_TEST_BUILD/workspace/lottesuper2011/branches/REAL/lotte-super-commons/dist/*.jar-->
			<fileset dir="${comm.wrksp.path}/${svnReposNm}/${hdsnBranchPath}/${comm.jar.path}">
				<include name="*.jar" />
			</fileset>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_COMMONS_1_TEST_BUILD/workspace/lottesuper2011/branches/REAL/lotte-super-winwin/WebContent/WEB-INF/lib/*.jar-->
			<fileset dir="${hdsnWrkSpPath}/${svnReposNm}/${hdsnBranchPath}/${srcLibPath}">
				<include name="*.jar" />
			</fileset>
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/branches/REAL/lotte-super-winwin/WebContent/WEB-INF/classes -->
			<dirset dir="${srcStaticClassesPath}" />
			<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/branches/REAL/lotte-super-winwin/WebContent/WEB-INF/dynamic_classes -->
			<dirset dir="${srcDynamicClassesPath}" />
		</path>
		
		<!-- static -->
		<!-- static class 디렉토리 생성후 Java 파일 컴파일 -->
		<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/branches/REAL/lotte-super-winwin/WebContent/WEB-INF/classes -->
		<!-- E:/jenkins-lottesuper/jobs/LOTTESUPER_WINWIN_1_TEST_BUILD/workspace/lottesuper2011/branches/REAL/lotte-super-winwin/src_dynamic -->
	    <mkdir dir="${srcStaticClassesPath}" />
		<javac source="1.7" 
			target="1.7" 
			srcdir="${srcStaticJavaPath}"
			classpathref="classpath"
			destdir="${srcStaticClassesPath}"
			failonerror="on" 
			debug="on"
			encoding="utf-8"
			compiler="modern"
			fork="true" 
			memoryMaximumSize="256m"
			includeantruntime="false"/>
			
