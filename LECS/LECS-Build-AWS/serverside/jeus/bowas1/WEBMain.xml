		</webserver-connection>
		<encoding>
			<request-url-encoding>
				<default>UTF-8</default>
			</request-url-encoding>
			<request-encoding>
				<default>UTF-8</default>
			</request-encoding>
			<response-encoding>
				<default>UTF-8</default>
			</response-encoding>
		</encoding>
		<logging>
			<access-log>
				<handler>
					<file-handler>
						<name>handler1</name>
						<valid-day>1</valid-day>
					</file-handler>
				</handler>
			</access-log>
		</logging>
	</context-group>
</web-container>
