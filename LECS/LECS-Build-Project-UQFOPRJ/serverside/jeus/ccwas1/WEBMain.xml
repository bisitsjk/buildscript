            <webtob-listener>
                <listener-id>webtob1</listener-id>
                <port>9900</port>
                <output-buffer-size>8192</output-buffer-size>
                <thread-pool>
                   <min>50</min>
                   <max>50</max>
                   <step>1</step>
                   <max-idle-time>30000</max-idle-time>
                </thread-pool>
                <webtob-address>121.254.239.30</webtob-address>
                <disable-pipe>true</disable-pipe>
                <webtob-home>/usr2/webtob</webtob-home>
                <registration-id>lecsssl</registration-id>
            </webtob-listener>
